  "appWorkflow": {
    "description": "Complete user journey through PhishGuard application",
    "flow": [
      {
        "step": 1,
        "screen": "Splash Screen",
        "duration": "2.5s",
        "action": "Show logo animation and check authentication state"
      },
      {
        "step": 2,
        "screen": "Authentication Check",
        "condition": "User logged in?",
        "ifTrue": "Navigate to Home Screen (step 4)",
        "ifFalse": "Navigate to Login Screen (step 3)"
      },
      {
        "step": 3,
        "screen": "Login/Register",
        "actions": ["User can login with credentials", "User can navigate to register", "After successful auth, navigate to Home (step 4)"]
      },
      {
        "step": 4,
        "screen": "Home Screen",
        "features": ["URL analysis", "View stats", "Recent scans", "Navigate via tabs"]
      },
      {
        "step": 5,
        "screen": "History Screen",
        "features": ["Filter scans", "Delete scans", "View details"]
      },
      {
        "step": 6,
        "screen": "Settings Screen",
        "features": ["View profile", "View stats", "Logout"]
      }
    ]
  },
