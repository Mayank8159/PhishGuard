      "h2": "24px Bold",
      "h3": "18px SemiBold",
      "body": "14-16px Regular",
      "button": "16px Bold, Letter Spacing 0.5"
    },
    "spacing": {
      "small": 8,
      "medium": 16,
      "large": 24,
      "xlarge": 32
